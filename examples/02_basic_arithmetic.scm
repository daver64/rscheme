; Example 2: Basic Arithmetic
; Working with numbers and arithmetic operations

; Addition
(display "Addition: ")
(display (+ 5 3))
(newline)

; Subtraction
(display "Subtraction: ")
(display (- 10 4))
(newline)

; Multiplication
(display "Multiplication: ")
(display (* 6 7))
(newline)

; Division
(display "Division: ")
(display (/ 20 4))
(newline)

; Multiple operands
(display "Sum of many: ")
(display (+ 1 2 3 4 5))
(newline)

; Nested expressions
(display "Nested: ")
(display (+ (* 2 3) (- 10 5)))
(newline)
